[ req ]
default_bits = 4096
prompt = no
default_md = sha384
x509_extensions = v3_req
distinguished_name = req_distinguished_name

[ req_distinguished_name ]
C = US
ST = MD
L = Test
OU = Test
CN = localhost

[ v3_req ]
keyUsage = keyEncipherment, dataEncipherment, digitalSignature
extendedKeyUsage = serverAuth, clientAuth
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = localhost
DNS.2 = devbox
DNS.3 = proxy_traefik
DNS.4 = example_app
IP.1 = 127.0.0.1
IP.2 = 192.168.0.173
IP.3 = 192.168.100.1
IP.4 = 192.168.100.100
IP.5 = 192.168.100.120
